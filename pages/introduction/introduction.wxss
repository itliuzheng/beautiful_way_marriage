/* pages/myself/introduction/introduction.wxss */
page{
  background: #F7F7F7;
}
.content-box{
  padding-bottom: 40rpx;
}
.content-topic{
  height: 448rpx;
  width: 100%;
  position: relative;
}
.header_mask{
  width: 100%;
  height:60rpx;
  background:rgba(0,0,0,0.8);
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
}
.header_mask::after{
  content:'';
  position: absolute;
  width:4rpx;
  height:28rpx;
  background:rgba(255,255,255,0.5);
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.mask_list{
  flex: 1;
  text-align: center;
  font-size:24rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,0.5);
  line-height:60rpx;
}
.mask_list .mask_icon{
  width: 36rpx;
  height: 32rpx;
  display: inline-block;
  margin-right: 20rpx;
  vertical-align: middle;
}
.mask_icon_transform{
  transition: 1s all ease;
  animation: love 2s 1;
} 

@keyframes love{
  0%{
    scale:1
  }
  50%{
    scale: 2
  }
  100%{
    scale: 1
  }
}

.mask_list .mask_icon_message{
  width: 34rpx;
  height: 32rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}
.header_bj{
  background: #ddd;
  width: 100%;
  height: 100%;
}
.header_bj image{
  display: block;
  height: 100%;
  width: 100%;
}

/* 个人介绍 */
.card-content{
  padding: 20rpx 40rpx 0 52rpx;
}
.move_title {
  font-size: 28rpx;
  color: #000;
  display: flex;
  align-items: center;
  line-height: 40rpx;
}
.move_miniTitle {
  font-size: 20rpx;
  margin-left: auto;
  position: relative;
  color: #C2A753;
}
.move_miniTitle::before{
  content:'';
  width:8rpx;
  height:8rpx;
  background:#C2A753;
  position: absolute;
  left: -20rpx;
  top: 50%;
  border-radius: 50%;
  transform: translate(0,-50%);
}
.lz-icon{
  position: relative;
  width: 28rpx;
  height: 28rpx;
  margin: 0 0 0 20rpx;
}
.icon-certified::before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;

}
.icon-not-certified:before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 28rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}
.card-content .name_other {
  font-size: 24rpx;
  color: #888888;
  margin-top: 12rpx;
}
.name_other text{
  margin-right: 10rpx;
}

.address image{
  width: 14rpx;
  height: 20rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 10rpx;
}
.address_desc{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(126,126,126,1);
  line-height:34rpx;
}
/* 个人介绍-end */

/* 动态 */
.detail{
  padding: 40rpx 0 0 62rpx;
}
.detail_type{
 margin-right: 60rpx; 
 position: relative;
}
.contact_tbn{
  position: absolute;
  width: 100%;
  height: 100%;
  background: transparent;
  left: 0;
  top: 0;
}
.detail_type image{
  width: 90rpx;
  height: 90rpx;
  display: block;
}
.detail_name{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(43,43,43,1);
  line-height:34rpx;
  margin-top: 20rpx;
}

/* 动态-end*/
.section{
  padding-bottom: 40rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.nav_head{
  padding: 20rpx 40rpx 0;
  border-bottom: 2px solid #F7F7F7;
  height: 104rpx;
}
.nav_head .fl{
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(196,171,91,1);
  line-height:84rpx;
}
.nav_head .fr{
  font-size:24rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:400;
  color:#7E7E7E;
  line-height:84rpx;
}
.list{
  height: 100rpx;
  border-bottom: 2px solid #F7F7F7;
  padding:0 40rpx;
}
.list .fl{
  font-size: 32rpx;
  line-height: 100rpx;
  color: #2B2B2B;
}
.list .fr{
  font-size: 28rpx;
  line-height: 100rpx;
  color: #888;
}
.arrow-box{
  line-height: 100rpx;
}
.arrow_b{
  vertical-align: top;
  display: inline-block;
  margin-left: 10rpx;
  width: 30rpx;
  height: 100rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 16rpx;
  transition: 0.5s all ease;
  transform: rotateZ(0);
}
.arrow_t{
  transform: rotateZ(-180deg);
}
.ver_show{
  height: 230rpx;
  width: 100%;
  background: #F7F7F7;
  padding-right: 40rpx;
  transition: 0.5s all ease;
  overflow: hidden;
}
.ver_show view{
  text-align: right;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  margin-top: 20rpx;
  
}


.show-tag{
  padding: 30rpx 40rpx 0;
}
.show-tag .tag{
  display: inline-block;
  font-size:24rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  padding: 14rpx 20rpx;
  background:rgba(247,247,247,1);
  border-radius:10rpx;
  border:2rpx solid rgba(151,151,151,1);
  margin-right: 30rpx;
  margin-bottom: 20rpx;
}
.section-show{
  padding: 20rpx 40rpx 0;
}
.section-show .show-text{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(182,182,182,1);
  line-height:40rpx;
}


.upload-box{
  font-size: 0;
  padding: 20rpx 40rpx 0;
}
.upload-list{
  margin-right: 22rpx;
  width: 210rpx;
  height: 200rpx;
  position: relative;
  display: inline-block;
}
.upload-list image{
  display: block;
  width: 100%;
  height: 100%;
}
.upload-list:nth-child(3n){
  margin-right: 0;
}

.upload-list-more{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.5);
  text-align: center;
  padding-top: 70rpx;
}
.upload-list-more .number{
  font-size:48rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height: 1;
  margin-bottom: 10rpx;
}
.upload-list-more .more{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height: 1;
}

.upload-list-m-b{
  margin-bottom: 20rpx;
}


.btn{
  width:280rpx;
  height:80rpx;
  line-height: 80rpx;
  margin: 60rpx auto 0;
  background:rgba(194,167,83,1);
  border-radius:16rpx;
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
}




.maskCode{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.8);
  display: flex;
  align-items: center;
  justify-content: center;
}
.toast_centerCode{
  position: relative;
  width: 686rpx;
  height: 678rpx;
  border-radius:10rpx;
  background-color: rgba(255, 255, 255, 1);
  box-sizing: border-box;
  padding:60rpx 40rpx 40rpx 40rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.toat_mainCode{
    width: 100%;
  height: 400rpx;
  font-size:28rpx;
  color: RGBA(153, 153, 153, 1);
  text-align: center;
}
.toat_mainCode text{
  display: block;
  margin: 0 auto;
}
.toat_mainCode image{
  width: 282rpx;
  height: 282rpx;
  display: block;
  margin: 100rpx auto 0;
}
.mask_close{
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  right: -24rpx;
  top: -24rpx;
  z-index: 99;
}