/* pages/myself/setting/setting.wxss */
.setting-view{
  padding-top: 30rpx;
}
.list-cell{
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 4rpx solid #F7F7F7;
  padding: 0 35rpx 0 40rpx;
  position: relative;
}
.list-cell::before{
  content: "";
  position: absolute;
  right: 40rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 19rpx;
}
.list-cell-none::before{
  background: none;
}
.list-cell text{
  font-size: 32rpx;
  color: #2B2B2B;
  font-weight: 400;
}


.maskCode{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.8);
  display: flex;
  align-items: center;
  justify-content: center;
}
.toast_centerCode{
  position: relative;
  width: 686rpx;
  height: 678rpx;
  border-radius:10rpx;
  background-color: rgba(255, 255, 255, 1);
  box-sizing: border-box;
  padding:60rpx 40rpx 40rpx 40rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.toat_mainCode{
    width: 100%;
  height: 400rpx;
  display: flex;
  font-size:28rpx;
  color: RGBA(153, 153, 153, 1);
}
.toat_main{
  width: 100%;
  height: 220rpx;
  display: flex;
  align-items: center;
  justify-content:center;
  font-size:32rpx;
  color: RGBA(0, 0, 0, 1);
}
.toat_yes{
  width: 100%;
  height:88rpx;
  font-size: 32rpx;
  color: RGBA(255, 255, 255, 1);
  display: flex;
  padding: 0;
  align-items: center;
  justify-content: center;
  border-radius: 5rpx;
  background-color: RGBA(174, 156, 128, 1);
}
.toat_no{
  color: RGBA(174, 156, 128, 1);
  background: RGBA(255, 255, 255, 1);
}
