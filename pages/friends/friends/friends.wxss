page{
  background: #F7F7F7;
}
.no-data{
  line-height: 100rpx;
  text-align: center;
  font-size: 24rpx;
}
.recommend-list{
  width: 100%;
  padding: 40rpx;
  background: #fff;
  margin-bottom: 20rpx;
}
.recommend-list .list-bj{
  width: 100%;
  height: 400rpx;
  position: relative;
}
.list-bj image{
  display: block;
  width: 100%;
  height: 400rpx;
}
.list-bj .certification_tag{
  padding: 0 20rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 0;
  color: #fff;
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  background:rgba(38,39,40,0.8);
  border-radius: 20rpx;
}
.list-bj .exchange_wx{
  left: auto;
  top: auto;
  right: 20rpx;
  bottom: 20rpx;
}
.certification_tag text{
  font-size: 20rpx;
  margin-left: 20rpx;
  vertical-align: top;
}
.certification_tag .icon_ver{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100%;
  margin-left: 0;
  margin-top: 8rpx;
}
.certification_tag .icon_ver_not{
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100%;
  margin-left: 0;
  margin-top: 8rpx;
}
.certification_tag .icon_wx{
  display: inline-block;
  width: 30rpx;
  height: 24rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100%;
  margin-top: 8rpx;
  margin-left: 0;
}
.card-content{
  margin-top: 20rpx;
}

.move_title {
  font-size: 28rpx;
  color: #000;
  display: flex;
  align-items: center;
  line-height: 40rpx;
}
.move_miniTitle {
  font-size: 20rpx;
  margin-left: auto;
  position: relative;
  color: #C2A753;
}
.move_miniTitle::before{
  content:'';
  width:8rpx;
  height:8rpx;
  background:#C2A753;
  position: absolute;
  left: -20rpx;
  top: 50%;
  border-radius: 50%;
  transform: translate(0,-50%);
}
.lz-icon{
  position: relative;
  width: 28rpx;
  height: 28rpx;
  margin: 0 0 0 20rpx;
}
.icon-certified::before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;

}
.icon-not-certified:before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 28rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}
.card-content .name_other {
  font-size: 24rpx;
  color: #888888;
  margin-top: 12rpx;
}
.name_other text{
  margin-right: 10rpx;
}

.name_introduct{
  margin-top: 20rpx;
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(182,182,182,1);
  line-height:40rpx;
  
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.go_more{
  text-align: right;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(192,164,76,1);
  line-height:34rpx;
  margin-top: 10rpx;
}