.video_box{
  width: 100%;
  background: #000;
}
video{
  width: 100%;
}


.commit-box{
  padding: 0 40rpx;
}
.title{
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:bold;
  color:rgba(0,0,0,1);
  line-height:44rpx;
  padding: 40rpx 0;
}
.comment_input{
  width:410rpx;
  height:68rpx;
  background:rgba(233,233,233,1);
  border-radius:34rpx;
  position: relative;
  float: left;
}
.input_icon{
  display: block;
  width: 32rpx;
  height: 68rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 30rpx;
  margin-left: 30rpx;
}
.input-style{
  position: absolute;
  width: 320rpx;
  height: 100%;
  padding-left: 74rpx;
  left: 0;
  top: 0;
  font-size: 24rpx;
}


.commit-icon{
  display: block;
  margin-left: 40rpx;
  line-height: 34rpx;
  margin-top: 24rpx;
}

.commit-icon .message{
  width: 32rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.commit-icon .collect{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.commit-icon .collectend{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.commit-icon text{
  margin-left: 10rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(185,185,185,1);
  line-height:34rpx;
}


.list{
  overflow: hidden;
  padding: 40rpx;
  border-bottom: 2rpx solid #F7F7F7;
}
.user_icon{
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: -100%;
  position: relative;
  z-index: 10;
}
.user_icon image{
  display: block;
  width: 100%;
  height: 100%;
  background: #999;
}
.single-content{
  padding-left: 108rpx;
  width: 100%;
}
.single_name{
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:bold;
  color:rgba(43,43,43,1);
  line-height:40rpx;
  margin-top: 4rpx;
  margin-bottom: 10rpx;
  text-align: left;
}
.content{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(43,43,43,1);
  line-height:40rpx;
  margin-top: 30rpx;
  margin-bottom: 46rpx;
}

.time-box{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
}
.time{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  text-align: left;
}
.icons text{
  display: inline-block;
  vertical-align: top;
}
.icons .icon_dianzan{
  display: inline-block;
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
  margin-right: 14rpx;
}
.icons .icon_dianzan_ok{
  background-image: url(data:image/png;base64,) 
}
.icons .icon_message{
  display: inline-block;
  width: 32rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 32rpx;
  margin: 0 18rpx 0 30rpx;
}
.icons .icon_share{
  display: inline-block;
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 28rpx;
  margin: 0 14rpx 0 22rpx;
}
.icons{
  position: relative;
}
.icons-button{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding: 0;
  min-width: auto;
  min-height: auto;
  background: transparent;
}
