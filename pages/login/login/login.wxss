page{
  background: #000000;
}
.logo-box{
  padding: 136rpx 80rpx 0;
}
.logo-box .english{
  font-size:96rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:1;
}
.logo-box .chinese{
  font-size:42rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(196,171,91,1);
  line-height:58rpx;
}

.login-box{
  margin-top: 242rpx;
  padding: 0 80rpx;
  text-align: center;
}
.login-box .input-cell{
  width: 100%;
  position: relative;
  height: 86rpx;
}
.input-cell input{
  border: none;
  border-bottom: 2rpx solid #888888;
  text-align: left;
  line-height: 1;
  padding: 14rpx 0  0 56rpx;
  color: #888888;
}
.login-box .phone-btn{
  width:100%;
  height:100rpx;
  background:rgba(196,171,91,1);
  border-radius:10rpx;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:100rpx;
  padding: 0;
  margin-top: 98rpx;
}
.desc{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  margin-top: 20rpx;
}
.desc .go_register{
  display: inline-block;
  margin-right: 20rpx;
}
.desc .go_forget{
  display: inline-block;
  margin-left: 20rpx;
}