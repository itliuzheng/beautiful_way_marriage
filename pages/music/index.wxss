.home_nav{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  padding: 40rpx 0;
  text-align: center;
  background: #fff;
  z-index: 999;
}
.nav_dom{
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(0,0,0,0.3);
  line-height:44rpx;
  display: inline-block;
  margin-right: 120rpx;
}
.nav_dom:last-child{
  margin-right: 0;
}
.nav_dom.active{
  color:rgba(196,171,91,1);
  border-bottom: 4rpx solid #C4AB5B;
}

.content{
  padding-top: 126rpx;
}

.music .list{
  height: 142rpx;
  width: 100%;
  border-bottom: 2px solid #F7F7F7; 
}

.list_number{
  width: 112rpx;
  margin-right: -100%;
  padding-right: 30rpx;
  text-align: right;
  font-size: 48rpx;
  line-height: 142rpx;
  color:rgba(136,136,136,1);
  font-weight: 500;
}
.list-content{
  margin-left: 112rpx;
  padding-top: 28rpx;
}

.title{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(43,43,43,1);
  line-height:40rpx;
  margin-bottom: 14rpx;
}
.icon-box{
  display: inline-block;
  margin-right: 40rpx;
  line-height: 34rpx;
  vertical-align: middle;
}

.icon-box .player{
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.icon-box .message{
  width: 32rpx;
  height: 28rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.icon-box .collect{
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.icon-box .collectend{
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.icon-box text{
  margin-left: 10rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(185,185,185,1);
  line-height:34rpx;
}

.video_list{
  padding: 0 40rpx;
  margin-bottom: 20rpx;
}
.video_list video{
  width: 100%;
}
.video_list .type-icon{
  text-align: right;
}