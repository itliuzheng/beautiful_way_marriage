/**index.wxss**/
.myself{
  min-height: 1334rpx;
  height: 100%;
  background: #fafafa;
}
.userinfo{
  width: 100%;
  height: 400rpx;
}
.no_user{
  background: #E2E2E2;
  position: relative;
}
.no_user image{
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 342rpx;
  height: 290rpx;
  transform: translate(-50%,0);
}
.my-bj{
  display: block;
  width: 100%;
  height: 400rpx;
}

.list-cell{
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 4rpx solid #F7F7F7;
  padding-left: 40rpx;
  position: relative;
}
.list-cell:before{
  content: "";
  position: absolute;
  right: 30rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 19rpx;
}
.list-cell text{
  font-size: 32rpx;
  color: #2B2B2B;
  font-weight: 400;
}


.my-info .list-cell{
  height: 178rpx;
  line-height: 178rpx;
  border: none;
  margin-bottom: 20rpx;
}
.my-info .list-cell .my_detail{
  padding-top: 40rpx;
}
.my_detail .my_name{
  font-size: 32rpx;
  color: #2B2B2B;
  line-height: 44rpx;
  font-weight: bold;
  margin-bottom: 14rpx;
}
.my_name text{
  font-size: 24rpx;
}
.my_detail .my_desc{
  font-size: 28rpx;
  color: #2b2b2b;
  line-height: 1;
  font-weight: 400;
}
.my_desc text{
  margin-right: 30rpx;
}
.my-info .list-cell .modify_detail{
  font-size: 24rpx;
  margin-right: 64rpx;
  color: #888; 
}


.lz-icon{
  position: relative;
  width: 28rpx;
  height: 28rpx;
  margin: 0 0 0 20rpx;
  vertical-align: top;
  display: inline-block;
  margin-top: 10rpx;
}
.icon-certified::before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 30rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;

}
.icon-not-certified:before{
  content:'';
  position: absolute;
  left: 0;
  top: 0;
  width: 28rpx;
  height: 28rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100%;
}