
.list-cell{
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  border-bottom: 4rpx solid #F7F7F7;
  padding:0 40rpx;
  position: relative;
}
.list-cell:before{
  content: "";
  position: absolute;
  right: 30rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 19rpx;
}
.list-cell-no_arrow:before{
  background: none;
}
.list-cell text{
  font-size: 32rpx;
  color: #2B2B2B;
  font-weight: 400;
}
.list-cell .fr{
  font-size: 28rpx;
  color: #888888;
}
.list-cell .fr_desc{
  padding-right: 20rpx;
}
.list-cell .fr_switch{
  margin-right: -20rpx;
}

.from-cell-head{
  height: 142rpx;
  line-height: 142rpx;
  padding: 0 40rpx;
}
.cell_head{
  font-size: 32rpx;
  color: #C4AB5B;
}
.cell_subhead{
  font-size: 24rpx;
  color: #C4AB5B;
}

.btn-form{
  text-align: center;
  margin: 100rpx 0 120rpx;

}
.btn{
  display: inline-block;
  width:200rpx;
  height:80rpx;
  border-radius:10rpx;
  border:2rpx solid rgba(196,171,91,1);
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(196,171,91,1);
  line-height:80rpx;
  text-align: center;
}
.btn_white{
  margin-right: 80rpx;
}
.btn_fill{
  background: #C4AB5B;
  color: #fff;
}