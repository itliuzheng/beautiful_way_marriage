.no-data{
  line-height: 200rpx;
  text-align: center;
}
.rc_swiper swiper{
  height: 258rpx;
}
.swiper-item{
  width: 100%;
  height: 320rpx;
}
.swiper-item image{
  height: 100%;
  width: 100%;
  display: block;
}
.add-single{
  width: 100rpx;
  height: 100rpx;
  position: fixed;
  bottom: 66rpx;
  right: 50rpx;
  z-index: 999;
}
.add-single image{
  width: 100rpx;
  height: 100rpx;
  display: block;
}

.list{
  overflow: hidden;
  padding: 40rpx;
  border-bottom: 2rpx solid #F7F7F7;
}
.user_icon{
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: -100%;
}
.user_icon image{
  display: block;
  width: 100%;
  height: 100%;
  background: #999;
}
.single-content{
  padding-left: 108rpx;
  width: 100%;
}
.name{
  font-size:28rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:bold;
  color:rgba(43,43,43,1);
  line-height:40rpx;
  margin-top: 4rpx;
}
.content{
  font-size:28rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(43,43,43,1);
  line-height:40rpx;
  margin-top: 16rpx;
}
.address{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  margin: 10rpx 0 24rpx;
}

.time-box{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
}
.icons text{
  display: inline-block;
  vertical-align: top;
}
.icons .icon_dianzan{
  display: inline-block;
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
  margin-right: 14rpx;
}
.icons .icon_dianzan_ok{
  background-image: url(data:image/png;base64,) 
}
.icons .icon_message{
  display: inline-block;
  width: 32rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 32rpx;
  margin: 0 18rpx 0 30rpx;
}
.icons .icon_share{
  display: inline-block;
  width: 34rpx;
  height: 34rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 28rpx;
  margin: 0 14rpx 0 22rpx;
}

.comment{
  width: 100%;
  background:rgba(247,247,247,1);
  padding: 20rpx;
  margin-top: 22rpx;
}
.comment-list{
  color: #1E1E1E;
  font-size: 24rpx;
  line-height: 34rpx;
  margin-bottom: 10rpx;
}
.comment-list .user_name{
  color: #C2A753;
  font-size: 24rpx;
  line-height: 34rpx;
}
.comment_more{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(194,167,83,1);
  line-height:34rpx;
  text-align: right;
  margin-top: 20rpx;
}

.single-img{
  margin-top: 20rpx;
  display: inline-block;
  margin-right: 10rpx;
}
.single-img:nth-child(3n){
  margin-right: 0;
}
.single-img image{
  display: block;
  width: 180rpx;
  height: 180rpx;
}
.single-img-one{
  display: block;
  width: 100%;
  max-height: 400rpx;
  margin-right: 0;
}
.single-img-one image{
  width: 100%;
  height: 400rpx;
  display: block;
}



.comment_input{
  width: 100%;
  height: 50px;
  display: flex;
  padding-top: 10rpx;
  background-color: #fff;
  position: fixed;
  bottom:0;
  z-index: 1000;
}
.form{
  width: 94%;
  margin:0 auto;
}
.input-style{
  width: 70%;
  height: 40px;
  border-radius: 25px;
  background: #f3f3f5;
  font-size: 14px;
  /* text-indent: 15px; */
  padding-left: 15px;
  float: left;
  color:#808286;
}
 
.btn{
  width: 20%;
  max-width: 20%;
  font-size: 14px;
  float: right;
  border-radius: 10px;
  color: #2c3036;
  background: #f3f3f5;
  letter-spacing: 2px;
  padding:0;
  line-height: 40px;
}