
.btn{
  display: inline-block;
  width:200rpx;
  height:80rpx;
  border-radius:10rpx;
  border:2rpx solid rgba(196,171,91,1);
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(196,171,91,1);
  line-height:80rpx;
  text-align: center;
}
.btn_white{
  display: block;
  margin: 76rpx auto 0;
  padding: 0;
  background: transparent;
}
.btn_fill{
  background: #C4AB5B;
  color: #fff;
  padding: 0;
}
.btn-form{
  text-align: right;
  padding-right: 20rpx;
}

.page{
  padding-top: 40rpx;
}
.push{
  margin-top: 40rpx;
  padding: 0 60rpx;
}
.push-text{

}
.push_imgs{
  margin-top: 160rpx;
  border-bottom: 2rpx solid #F7F7F7;
  padding: 0 60rpx 30rpx;
}


.upload-box{
  font-size: 0;
}
.upload-list{
  margin-right: 14rpx;
  width: 200rpx;
  height: 200rpx;
  position: relative;
  display: inline-block;
  background: #000;
}
.upload-list image{
  display: block;
  width: 100%;
  height: 100%;
}
.upload-list:nth-child(3n){
  margin-right: 0;
}
.upload-list-m-b{
  margin-bottom: 10rpx;
  vertical-align: top;
}

.add{
  width: 200rpx;
  height: 200rpx;
  display: inline-block;
  background: #D8D8D8;
  position: relative;
}
.add .horizontal{
  position: absolute;
  width: 64rpx;
  height: 6rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background: #fff;
}
.add .vertical{
  position: absolute;
  width: 6rpx;
  height: 64rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background: #fff;
}



.list-cell{
  height: 124rpx;
  line-height: 124rpx;
  background: #fff;
  border-bottom: 2rpx solid #F7F7F7;
  padding:0 40rpx 0 60rpx;
  position: relative;
}
.list-cell:before{
  content: "";
  position: absolute;
  right: 40rpx;
  top: 0;
  width: 11rpx;
  height: 100%;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% 19rpx;
}
.list-cell text{
  font-size:32rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(0,0,0,1);
}
.address-icon{
  width: 30rpx;
  height: 40rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 20rpx;
}