page{
  background: #000000;
}
.page-bj{
  display: block;
  width: 750rpx;
  height: 1160rpx;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.page-view{
  position: relative;
  z-index: 2;
}

.logo-box{
  padding: 136rpx 80rpx 0;
}
.logo-box .english{
  font-size:96rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:1;
}
.logo-box .chinese{
  font-size:42rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(196,171,91,1);
  line-height:58rpx;
}

.login-box{
  margin-top: 242rpx;
  padding: 0 80rpx;
  text-align: center;
}
.login-box .input-cell{
  width: 100%;
  position: relative;
  height: 86rpx;
  padding-top: 30rpx;
}
.input-cell input{
  border: none;
  border-bottom: 2rpx solid #888888;
  text-align: left;
  height: 56rpx;
  line-height: 56rpx;
  padding: 0 0  0 56rpx;
  color: #888888;
  box-sizing: border-box;
}
.login-box .phone-btn{
  width:100%;
  height:100rpx;
  background:rgba(196,171,91,1);
  border-radius:10rpx;
  font-size:32rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height:100rpx;
  padding: 0;
  margin-top: 98rpx;
}
.desc{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(136,136,136,1);
  line-height:34rpx;
  margin-top: 20rpx;
}
.desc .go_register{
  display: inline-block;
  margin-right: 20rpx;
}
.desc .go_forget{
  display: inline-block;
  margin-left: 20rpx;
}

.input-icon{
  position: absolute;
  left: 0;
  top: 14rpx;
}
.icon-phone{
  width: 24rpx;
  height: 86rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100% 32rpx;
}
.icon-password{
  width: 28rpx;
  height: 86rpx;
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 100% 32rpx;
}


.maskCode{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.8);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 100;
}
.toast_centerCode{
  position: relative;
  width: 686rpx;
  border-radius:10rpx;
  background-color: rgba(255, 255, 255, 1);
  box-sizing: border-box;
  padding:60rpx 40rpx 40rpx 40rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.toat_mainCode{
    width: 100%;
  font-size:28rpx;
  color: RGBA(153, 153, 153, 1);
  text-align: center;
}
.toat_mainCode text{
  display: block;
  margin: 0 auto;
}
.mask_close{
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  right: -24rpx;
  top: -24rpx;
  z-index: 99;
}